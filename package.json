{
  "name": "fullbosts-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json,html}\" ",
    "lint": "eslint ./src --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,less}": [
      "prettier --ignore-path ./.prettierignore --write "
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@innei-util/eslint-config-react-ts": "^0.8.2",
    "@innei-util/prettier": "^0.8.2",
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.25",
    "postcss-import": "15.1.0",
    "prettier": "3.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6",
    "vite": "4.4.2"
  }
}